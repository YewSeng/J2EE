<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CommonLoginFilter</filter-name>
  	<display-name>CommonLoginFilter</display-name>
  	<description></description>
  	<filter-class>com.Caltech.servlet.CommonLoginFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CommonLoginFilter</filter-name>
  	<url-pattern>/CommonLogin</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>CommonLogin</servlet-name>
  	<display-name>CommonLogin</display-name>
  	<description></description>
  	<servlet-class>com.Caltech.servlet.CommonLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.Caltech.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CommonLogin</servlet-name>
  	<url-pattern>/CommonLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
</web-app>
